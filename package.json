{
    "name": "trepid-ink",
    "displayName": "Trepid Ink",
    "description": "A color theme, designed to mix well with vscode its interface. Inspired by Trepid",
    "version": "0.1.1",
    "publisher": "Squeeble",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Themes",
        "Other"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Trepid Ink",
                "uiTheme": "vs-dark",
                "path": "./themes/TrepidInk-color-theme.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SqueebleInk/VSC-THM-TrepidInk.git"
    },
    "bugs": {
        "url": "https://github.com/SqueebleInk/VSC-THM-TrepidInk/issues"
    },
    "icon": "images/logo.png",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.42.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "tslint": "^5.20.0",
        "vscode-test": "^1.2.2"
    }
}
